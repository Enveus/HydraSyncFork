{
  "name": "syncbot",
  "version": "1.1.0",
  "description": "A bot that syncs roles between one main server, and multiple other discord servers.",
  "main": "src/index.js",
  "bin": {
    "syncbot": "bin/syncbot.js"
  },
  "scripts": {
    "start": "node bin/syncbot.js start",
    "start:reverse": "node bin/syncbot.js start-reverse",
    "register": "node bin/syncbot.js register",
    "register:global": "node bin/syncbot.js register-global",
    "debug": "node bin/syncbot.js start --debug",
    "debug:reverse": "node bin/syncbot.js start-reverse --debug",
    "ui": "node src/web/start.js",
    "ui:server": "node src/web/server.js",
    "install-global": "npm install -g .",
    "uninstall-global": "npm uninstall -g syncbot",
    "start:ui": "node bin/syncbot.js start --ui",
    "start:ui:browser": "node bin/syncbot.js start --ui --open-browser",
    "start:reverse:ui": "node bin/syncbot.js start-reverse --ui",
    "start:reverse:ui:browser": "node bin/syncbot.js start-reverse --ui --open-browser",
    "ui:standalone": "node bin/syncbot.js ui"
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathonor/syncbot.git"
  },
  "keywords": [
    "discord",
    "sync",
    "bot",
    "server",
    "week"
  ],
  "author": "jonathonor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonathonor/syncbot/issues"
  },
  "type": "module",
  "homepage": "https://github.com/jonathonor/syncbot#readme",
  "dependencies": {
    "@discordjs/rest": "^1.0.0",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "discord-api-types": "^0.36.2",
    "discord.js": "^14.0.3",
    "express": "^4.21.2"
  }
}
